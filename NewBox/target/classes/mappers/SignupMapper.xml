<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.newbox.dao.SignupDAO">
	<select id="signupIdCheck" resultType="Integer">
		select
			count(*)
		from
			user_mst
		where
			user_id=#{id}	
	</select>
</mapper>